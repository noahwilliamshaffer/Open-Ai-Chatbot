{
  "name": "openai-api-integration",
  "version": "1.0.0",
  "description": "A project that integrates with OpenAI API and Firebase",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:cli": "node index.js",
    "docker:build": "docker build -t openai-firebase-app .",
    "docker:run": "docker run -p 3000:3000 -e OPENAI_API_KEY openai-firebase-app"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase": "^10.5.0",
    "openai": "^4.24.1"
  }
}
